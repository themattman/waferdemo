extends layout

block append content
  h1= title
  p Welcome to #{title}

  style.
    #chart svg {
      height: 400px;
    }
  #chart
    svg

  .box
    .status-svg
      svg(width="50px", height="50px")
        circle#status(cx="20", cy="20", r="10", fill="red")
    .crud-box
      table.table.table-striped.table-bordered.span4
        thead
          tr
            td key
            td value
        tbody.data-tbody

block append scripts
  script(src="//cdnjs.cloudflare.com/ajax/libs/d3/3.3.10/d3.min.js")
  script(src="//cdnjs.cloudflare.com/ajax/libs/nvd3/1.1.13-beta/nv.d3.min.js")
  script(src="/js/b.js")
  script(src="/js/compare1.js")